<UserControl x:Class="VolumeBoard"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:GMEdit_WPF"
             mc:Ignorable="d" 
             d:DesignHeight="100" d:DesignWidth="4096"
             Loaded="UserControl_Loaded" SizeChanged="UserControl_SizeChanged">
    <Grid Background="#505050" Name="mb">
        <StackPanel Height="1" Margin="0,20,0,0" VerticalAlignment="top" Background="#353535"></StackPanel>
        <StackPanel Height="1" Margin="0,40,0,0" VerticalAlignment="top" Background="#353535"></StackPanel>
        <StackPanel Height="1" Margin="0,60,0,0" VerticalAlignment="top" Background="#353535"></StackPanel>
        <StackPanel Height="1" Margin="0,80,0,0" VerticalAlignment="top" Background="#353535"></StackPanel>
    </Grid>
</UserControl>
